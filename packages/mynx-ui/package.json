{
  "name": "mynx-ui",
  "version": "1.0.1",
  "description": "Core UI components for Mynx UI",
  "main": "dist/mynx-ui.umd.js",
  "module": "dist/mynx-ui.es.js",
  "types": "types/index.d.ts",
  "homepage": "https://github.com/luna-lee/mynx.git",
  "exports": {
    ".": { 
      "types": "./types/index.d.ts",
      "import": "./dist/mynx-ui.es.js",
      "require": "./dist/mynx-ui.umd.js"
    },
    "./client": {
      "types": "./types/global.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "typesVersions": {
    "*": {
      "client": [
        "./types/global.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "rm -rf dist types && vite build && npm run build:types",
    "build:types": "vue-tsc --emitDeclarationOnly &&   tsx scripts/generateTypes.mts",
    "typecheck": "vue-tsc --noEmit",
    "npm:publish": "npm publish",
    "clean": "rm -rf dist types"
    
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@types/d3": ">=7.0.0",
    "@types/lodash-es": ">=4.17.6",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "prettier": "^3.5.3",
    "sass": "^1.87.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.3.3",
    "unocss": "66.1.0-beta.12",
    "vite": "^5.1.4",
    "vue": "^3.5.1",
    "vue-tsc": "^2.0.4"
  },
  "keywords": [
    "vue",
    "vue3",
    "component",
    "ui",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@vueuse/core": ">=11.0.0",
    "d3": ">=7.0.0",
    "async-validator": ">=4.2.5",
    "lodash-es": ">=3.0.0",
    "mynx-utils": "workspace:*"
  }
}
